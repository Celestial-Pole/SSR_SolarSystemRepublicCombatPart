<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="SSR_BaseWeapon">
    <defName>Electromagnetic_AR</defName>
    <label>电磁步枪</label>
    <description>地球联邦时代早期的制式突击步枪，相比于传统的火药步枪，使用电磁轨道加速弹丸的电磁步枪拥有更远的射程，也能对目标造成更大的伤害。</description>
    <graphicData>
      <texPath>Things/Weapons/Electromagnetic Weapon/Electromagnetic_AR</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_ChargeRifle</soundInteract>
    <statBases>
      <WorkToMake>45000</WorkToMake>
      <Mass>3.5</Mass>
      <AccuracyTouch>1.00</AccuracyTouch>
      <AccuracyShort>1.00</AccuracyShort>
      <AccuracyMedium>1.00</AccuracyMedium>
      <AccuracyLong>0.85</AccuracyLong>
      <RangedWeapon_Cooldown>1.50</RangedWeapon_Cooldown>
    </statBases>
    <recipeMaker>
      <researchPrerequisite>SSRResearchElectromagneticWeapon</researchPrerequisite>
      <recipeUsers>
        <li>SSR_NumericalControlledMachineTool</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
    </recipeMaker>
    <costList>
      <SSR_ArmourSteel>12</SSR_ArmourSteel>
      <SSR_StrongElectromagnet>3</SSR_StrongElectromagnet>
      <SSR_SuperconductingMaterial>1</SSR_SuperconductingMaterial>
      <ComponentIndustrial>10</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Electromagnetic_AR_rt</defaultProjectile>
        <warmupTime>0.5</warmupTime>
        <range>45</range>
        <ticksBetweenBurstShots>10</ticksBetweenBurstShots>
        <burstShotCount>3</burstShotCount>
        <soundCast>ElectromagneticLt</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <modExtensions>
			<li Class="FS_SSR.TCP_AbsoluteDamageAddon">
				<forceHit>true</forceHit>
				<absDmgAmt>-1</absDmgAmt>
        <researchReq>SSRResearchIntelligenceBullet</researchReq>
				<damageType>Bullet</damageType>
			</li>
		</modExtensions>
    <weaponTags>
      <li>SSR_MechanoidGunAR</li>
    </weaponTags>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>


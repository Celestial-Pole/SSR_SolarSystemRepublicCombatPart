			<researchPrerequisite>SSRPlasmaWeapon</researchPrerequisite>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Plasteel>135</Plasteel>
			<ComponentIndustrial>15</ComponentIndustrial>
			<ComponentSpacer>5</ComponentSpacer>
		</costList>
